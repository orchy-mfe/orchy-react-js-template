{
  "plugins": [],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "react-app"
  ],
  "rules": {
    "no-unused-vars": "error",
    "semi": [
      "error",
      "never"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "object-curly-spacing": [
      2,
      "never"
    ],
    "react/jsx-sort-props": [
      2,
      {
        "ignoreCase": true
      }
    ],
    "react/no-unused-prop-types": 2,
    "react/sort-prop-types": 2,
    "react/jsx-filename-extension": 2,
    "quote-props": [
      "error",
      "as-needed"
    ]
  }
}